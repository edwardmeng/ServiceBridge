<?xml version="1.0"?>
<package>
  <metadata minClientVersion="3.4">
    <id>ServiceBridge.ServiceModel.Activation</id>
    <version>1.0.0</version>
    <authors>edwardmeng</authors>
    <owners>edwardmeng</owners>
    <licenseUrl>http://www.opensource.org/licenses/mit-license.php</licenseUrl>
    <projectUrl>https://github.com/edwardmeng/ServiceBridge</projectUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>
      This library integrate ServiceBridge with MassActivation for WCF services.
    </description>
    <tags>IoC DependencyInjection WCF Interception AOP Activation</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.0" >
        <dependency id="MassActivation" version="[1.0,2.0)" />
        <dependency id="ServiceBridge" version="[1.0,2.0)" />
        <dependency id="ServiceBridge.ServiceModel" version="[1.0,2.0)" />
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.ServiceModel" targetFramework="net40"/>
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="lib\**\*.dll" target="lib" />
    <file src="lib\**\*.xml" target="lib" />
    <file src="..\LICENSE" />
  </files>
</package>