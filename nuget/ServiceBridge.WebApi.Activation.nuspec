<?xml version="1.0"?>
<package>
  <metadata minClientVersion="3.4">
    <id>ServiceBridge.WebApi.Activation</id>
    <version>1.0.0</version>
    <authors>edwardmeng</authors>
    <owners>edwardmeng</owners>
    <licenseUrl>http://www.opensource.org/licenses/mit-license.php</licenseUrl>
    <projectUrl>https://github.com/edwardmeng/ServiceBridge</projectUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>
      This library integrate ServiceBridge with MassActivation for ASP.NET WebApi 5 controllers.
    </description>
    <tags>IoC DependencyInjection WebApi Interception AOP Activation</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.5" >
        <dependency id="MassActivation" version="[1.0,2.0)" />
        <dependency id="ServiceBridge" version="[1.0,2.0)" />
        <dependency id="ServiceBridge.WebApi" version="[1.0,2.0)" />
        <dependency id="Microsoft.AspNet.WebApi" version="[5.0.0,6.0.0)" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="lib\**\*.dll" target="lib" />
    <file src="lib\**\*.xml" target="lib" />
    <file src="..\LICENSE" />
  </files>
</package>